{
    "name": "kepinski.me",
    "version": "1.0.0",
    "description": "Personal site",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "start": "next start",
        "test": "xo && stylelint './**/*.js'"
    },
    "engines": {
        "node": "12.x"
    },
    "xo": {
        "envs": [
            "node",
            "browser"
        ],
        "extends": "xo-react",
        "nodeVersion": ">=10",
        "parser": "babel-eslint",
        "rules": {
			"import/no-unassigned-import": "off",
			"import/extensions": "off",
			"unicorn/string-content": "off"
        },
        "settings": {
            "react": {
                "version": "16.12"
            }
        }
    },
    "dependencies": {
        "core-js": "^3.6.4",
        "framer-motion": "^1.10.0",
        "next": "^9.3.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-parallax-tilt": "^1.3.40",
        "react-simple-img": "^2.3.9",
        "styled-components": "^5.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "eslint-config-xo-react": "^0.23.0",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-svgo": "^7.1.0",
        "next-fonts": "^1.0.3",
        "next-offline": "^5.0.0",
        "next-optimized-images": "^2.5.5",
        "stylelint": "^13.2.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "xo": "^0.28.0"
    }
}
